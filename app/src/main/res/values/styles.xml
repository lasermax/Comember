<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="main_header">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/text_dark</item>
        <item name="android:textAllCaps">true</item>
        <item name="fontWeight">600</item>
    </style>
    <style name="normal_text">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text_dark</item>
        <item name="android:textAllCaps">true</item>
        <item name="fontWeight">400</item>
    </style>
    <style name="highscore_text">
        <item name="android:textSize">176sp</item>
        <item name="android:textColor">@color/background_contrast</item>
        <item name="android:textStyle">bold</item>
        <item name="fontWeight">800</item>
    </style>
</resources>